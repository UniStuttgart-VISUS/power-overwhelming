<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ImportGroup Label="PropertySheets" />
  <PropertyGroup Label="UserMacros">
    <VisaDir>$(ProgramFiles)\IVI Foundation\VISA\</VisaDir>
    <VisaPlatform Condition="Exists('$(VisaDir)WinNT')">WinNT</VisaPlatform>
    <VisaPlatform Condition="Exists('$(VisaDir)Win64')">Win64</VisaPlatform>
  </PropertyGroup>
  <PropertyGroup />
  <ItemDefinitionGroup>
    <ClCompile>
      <PreprocessorDefinitions Condition="Exists('$(VisaDir)$(VisaPlatform)')">POWER_OVERWHELMING_WITH_VISA;%(PreprocessorDefinitions)</PreprocessorDefinitions>
      <AdditionalIncludeDirectories>$(VisaDir)$(VisaPlatform)\Include;%(AdditionalIncludeDirectories)</AdditionalIncludeDirectories>
    </ClCompile>
  </ItemDefinitionGroup>
  <ItemGroup>
    <BuildMacro Include="VisaDir">
      <Value>$(VisaDir)</Value>
    </BuildMacro>
    <BuildMacro Include="VisaPlatform">
      <Value>$(VisaPlatform)</Value>
    </BuildMacro>
  </ItemGroup>
</Project>